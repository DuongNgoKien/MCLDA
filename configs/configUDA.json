{
    "model": "Segfomer",
    "dataset": "cityscapes",
  
    "training": {
      "batch_size": 2,
      "num_workers": 2,
      "optimizer": "AdamW",
      "momentum": 0.9,
      "num_iterations": 250000,
      "learning_rate": 1.5e-5,
      "lr_schedule": "Poly",
      "lr_schedule_power": 0.9,
      "weight_decay": 0.01,
      "use_sync_batchnorm": true,
  
      "data": {
        "split_id_list": 0,
        "labeled_samples": 0,
        "input_size": "512,512",
        "scale": false,
        "crop": true
      },
      "unlabeled": {
        "train_unlabeled": true,
        "consistency_weight": 1,
        "consistency_loss": "CE",
        "pixel_weight": "threshold_uniform",
        "mix_mask": "class",
        "flip": false,
        "color_jitter": true,
        "blur": true
      }
  
    },
    "seed": 1,
    "pretrained": "coco",
    "ignore_label": 255,
  
    "utils": {
      "save_checkpoint_every": 5000,
      "checkpoint_dir": "../saved/DeepLabv2",
      "val_per_iter": 5000,
      "tensorboard": true,
      "log_per_iter": 100,
      "save_best_model": true
   }
}
